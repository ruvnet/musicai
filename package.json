{
  "name": "musicai",
  "version": "1.0.0",
  "description": "High-performance AI-powered auto-tune and music enhancement system for Raspberry Pi 4. Corrects pitch, timing, and tonal quality in real-time using self-learning AI powered by ruvector.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "musicai": "./dist/cli.js"
  },
  "directories": {
    "doc": "docs"
  },
  "napi": {
    "name": "musicai",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-unknown-linux-gnu",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release && tsc",
    "build:debug": "napi build --platform && tsc",
    "build:rust": "napi build --platform --release",
    "build:ts": "tsc",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "demo": "npm run build:ts && npx tsx demo/full-system-demo.ts",
    "benchmark": "npm run build:ts && npx tsx demo/benchmark.ts",
    "benchmark:comprehensive": "npm run build:ts && npx tsx demo/comprehensive-audio-benchmark.ts",
    "benchmark:audio": "npm run build:ts && npx tsx demo/real-audio-test.ts",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "version": "napi version"
  },
  "repository": {
    "type": "git",
    "url": "http://local_proxy@127.0.0.1:51916/git/ruvnet/musicai"
  },
  "keywords": [
    "ai",
    "music",
    "auto-tune",
    "agent-swarm",
    "ruvector",
    "raspberry-pi"
  ],
  "author": "rUv",
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@ruvector/agentic-synth": "^0.1.5",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "eventemitter3": "^5.0.1",
    "ora": "^9.0.0",
    "p-queue": "^9.0.1",
    "zod": "^4.1.12"
  }
}
